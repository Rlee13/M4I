from M4I import Matrix3, Matrix4

O = Matrix3(0)
O.from_list([[1,1,1],
             [1,2,3],
             [4,3,2]])

print(O[1, 1])

S = Matrix4(1.0)

S.from_list([[1,1,1,2],
             [1,2,3,5],
             [4,3,2,6],
             [9,2,4,5]
             ])

print(S[2, 1])
print(S[1:2, 1:2])

T = Matrix3(0)
T.from_list([[10,10,10],
             [10,10,10],
             [10,10,10]])

S[1:3, 1:3] = T
print(S)

S[1:3, 1:3] = S[1:3, 1:3] + T
print(S)